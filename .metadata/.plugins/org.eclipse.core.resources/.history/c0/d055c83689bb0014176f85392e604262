$(document).ready(function (){
	var box = $("#angleBox");
	
	$("#stupid").click(function (){
		setAngle();
	});
	
	function setAngle(){
		var angle = parseInt(box.val());
		angle-=360;
		var radians = (Math.PI) * (angle/180) ;
		var radius = 250;
		var x = Math.cos(radians) * radius; 
		var y = Math.sin(radians) * radius; 
		
		document.getElementById("angle").setAttributeNS(null, "x2", x);
		document.getElementById("angle").setAttributeNS(null, "y2", y);
		
	}
	function onClickBut(){
		setAngle();
	}
	
	
	
});

