$(document).ready(function (){
	var box = $("#angleBox");
	
	$("#stupid").click(function (){
		setAngle();
	});
	
	function setAngle(){
		var angle = parseInt(box.val());
		var radians = (Math.PI * 2) * (angle % 360) ;
		var radius = 250;
		var x = Math.cos(radians) * radius + 50; 
		var y = Math.sin(radians) * radius + 50; 
		
		document.getElementById("angle").setAttributeNS(null, "x2", x);
		document.getElementById("angle").setAttributeNS(null, "y2", y);
		
	}
	
	function onClickBut(){
		setAngle();
	}
	
	
	
});

